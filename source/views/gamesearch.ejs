<% include head.ejs %>
<title>Add games</title>
</head>
<body>
	<header>
		<h1>Profile</h1>
		<h1>add game</h1>
	</header>
	<main>
		<!-- <a href="./games">back</a> -->
		<section>
			<% for (var index = 0; index <data.length; index++) { %>
			<article class="game">
				<h3><%= data[index].title %></h3>
				<img src=<%= data[index].img %>>
				<form class="" action="gamesearch/<%= data[index].id %>" method="post">
					<button type="submit" class="addIcon"></button>
				</form>
			</article>
			<% } %>
		</section>
	</main>
</body>
</html>
